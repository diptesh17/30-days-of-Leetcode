class Solution {
public:
        int lengthOfLongestSubstring(string s) {
        int maxilen=0,len=0,j=0;
      map<char,int> mp;
        for(int i=0;i<s.size();i++){
           mp[s[i]]++;
           len++;
           while(mp[s[i]]>1)
           {
               len--;
               mp[s[j++]]--;
           }
          maxilen=max(len,maxilen);
        }
        return maxilen;
    }
};
